{
    "name": "BetterSpider",
    "version": "1.0",
    "description": "chrome",
    "permissions": [],
    "offline_enabled": true,
    "author": "leeyong",
    "minimum_chrome_version": "110",
    "content_security_policy": {
        "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self';"
    },
    "action": {
        "default_icon": {
            "16": "assets/icon-16.png",
            "19": "assets/icon-19.png",
            "38": "assets/icon-38.png",
            "128": "assets/icon-128.png"
        },
        "default_title": "BetterSpider"
    },
    "icons": {
        "16": "assets/icon-16.png",
        "38": "assets/icon-38.png",
        "128": "assets/icon-128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "snippets/dioxus-interpreter-js-7c1300c6684e1811/inline0.js",
                "snippets/dioxus-interpreter-js-7c1300c6684e1811/src/js/common.js",
                "snippets/dioxus-web-84af743b887ebc54/inline0.js",
                "snippets/dioxus-web-84af743b887ebc54/inline1.js",
                "snippets/dioxus-web-84af743b887ebc54/src/eval.js",
                "chrome_lib.js",
                "run_wasm.js"
            ],
            "run_at": "document_end",
            "all_frames": false
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "chrome_bg.wasm"
            ],
            "matches": [
                "http://*/*",
                "https://*/*"
            ]
        }
    ],
    "manifest_version": 3
}